class HeirMyStack extends MyStack{
	private ArrayList<Object> list = new ArrayList<Object>();

	@Override
	public boolean isEmpty(){
		return list.isEmpty();
	}

	public void push(String o){
		list.add(o);
	}

	@Override
	public int getSize(){
		return list.size();
	}

	@Override
	public Object peek(){
		return list.get(getSize() - 1);
	}

	@Override
	public Object pop(){
		Object o = list.get(getSize()-1);
		list.remove(getSize()-1);
		return o;
	}
	
}
